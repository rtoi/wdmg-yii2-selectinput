{
  "name": "wdmg/yii2-selectinput",
  "description": "Bootstrap3 SelectInput widget for Yii2",
  "keywords": ["yii", "yii2", "extension", "select", "input", "bootstrap", "widget", "wdmg"],
  "type": "yii2-extension",
  "license": "MIT",
  "version": "1.0.4",
  "homepage": "https://github.com/wdmg/yii2-selectinput",
  "support": {
    "source": "https://github.com/wdmg/yii2-selectinput",
    "issues": "https://github.com/wdmg/yii2-selectinput/issues"
  },
  "authors": [
    {
      "name": "Alexsander Vyshnyvetskyy",
      "email": "alex.vyshnyvetskyy@gmail.com"
    },
    {
      "name": "W.D.M.Group, Ukraine",
      "email": "wdmg.com.ua@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.19",
    "yiisoft/yii2-bootstrap": "*",
    "bower-asset/bootstrap-selectinput-plugin": "^1.1.1"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "wdmg\\widgets\\": "src"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "bower-asset-library": "vendor/bower"
    }
  }
}
